{
  "hosting": {
    "site": "zoe-server-art",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "Cache-control": ["no-store","no-cache", "must-revalidate"],
        "Pragma": "no-cache",
        "Expires": 0
      }
    ]
  }
}
